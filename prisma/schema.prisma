generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Producao {
  id        String   @id @default(cuid())
  pedidoId  String   @unique
  status    String   @default("AGUARDANDO")
  updatedAt DateTime @updatedAt
  createdAt DateTime @default(now())
}
